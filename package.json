    "@material-ui/lab": "^4.0.0-alpha.32",
    "algoliasearch": "^3.35.1",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.2",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "material-ui-icons": "^1.0.0-beta.36",
    "material-ui-upload": "^1.2.1",
    "meteor-node-stubs": "^0.4.1",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-autosuggest": "^9.4.3",
    "react-chartjs-2": "^2.8.0",
